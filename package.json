{
  "name": "moodmetertec",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "test:e2e:mocks": "E2E_MOCKS=1 playwright test",
    "validate": "npm run lint && npm run typecheck && npm run test:unit",
    "validate:full": "npm run validate && npm run build",
    "db:migrate": "node scripts/run-migrations.cjs",
    "db:seed": "node scripts/run-seeds.cjs",
    "db:seed:catalogs": "node scripts/run-seeds-catalogs.cjs",
    "db:seed:mentors": "node scripts/run-seed-mentors.cjs"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "jose": "^5.9.6",
    "lucide-react": "^0.468.0",
    "next": "16.0.1",
    "next-auth": "^4.24.7",
    "nodemailer": "^6.9.16",
    "pg": "^8.12.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "recharts": "^2.15.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.2",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.16",
    "@types/pg": "^8.15.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^1.6.0"
  }
}
